
/* 右下角添加按钮 */
.add_button{
  position: fixed;
  right: 30rpx;
  bottom: 50rpx;
  width: 44pt;
  height: 44pt;
  background-color: #4B9EFD;
  border-radius: 50%;
  font-size: 26pt;
  color: #fff;
  box-shadow: 5rpx 6rpx 14rpx 1rpx rgba(74, 157, 252, 0.53);
}
.add_button navigator{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
